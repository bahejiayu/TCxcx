.withTop{
  height: 400rpx;
  background: #ffda43; 
  position: relative;
}
.TopCenter{
  text-align: center;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.TopCenter view:first-child{
  font-size: 60rpx;
  color: #333;
  margin-bottom: 5rpx;
}
.TopCenter view:first-child text{
  font-size: 30rpx;
}
.TopCenter view:last-child{
  font-size: 26rpx;
  color:  #333;
}
.all_get{
  color:  #333;
  font-size: 24rpx;
  height: 60rpx;
  padding: 0 18rpx;
  border: 1px solid  #333;
  border-radius: 10rpx;
  line-height: 60rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.bottomNav{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 108rpx;
}

.bottomItem{
  float: left;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333;
  width:33.33%;
  text-align: center;
}
.line_b{
  width: 33.33%;
  position: absolute;
  height: 8rpx;
  bottom: 0;
  left: 0;
  transition: all 0.4s;
  background-color: #ffb80f;
}
page{
  background-color: #eeeeee;
}
.toDay{
  height: 178rpx;
  line-height: 178rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #cccccc;
  /* margin-top: 10rpx; */
  padding: 0 25rpx;
}
.toDay view:first-child{
  float: left;
  font-size:30rpx ;
  color: #333;
}
.toDay view:last-child{
  float: right;
  font-size:32rpx ;
  color: #f9484d;
}
.moneyDetail{
  overflow: hidden;

}
.money_item{
  width: 33.33%;
  text-align: center;
  padding: 60rpx 0;
  float: left;
}
.money_item view:first-child{
  font-size: 44rpx;
  color:#ff7700 ;
}

.money_item view:last-child{
  font-size: 24rpx;
  margin-top: 15rpx;
  color:#666666 ;
}
.go_getnow{
  display: block;
  width:140rpx ;
  height:55rpx ;
  text-align: center;
  line-height: 55rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #0da056;
  border: 2rpx solid #0da056;
  border-radius: 10rpx;
  position: relative;
  z-index: 1000;
}
.nav_Detail{
    height: 92rpx;
    line-height: 92rpx;
    background-color: #fff;
    font-size: 30rpx;
    color:#333333 ;
    position: relative;

}

.nav_item{
  float: left;
  width: 50%;
  text-align: center;
}

.line_gr{
  position: absolute;
  width: 50%;
  bottom: 0;
  height: 4rpx;
  background-color: #0da056;
  left: 0;
  transition: all 0.2s;
}

.detail_tiem{
  height: 60rpx;
  line-height: 60rpx;
  padding: 0 25rpx;
  background-color: #f1f1f1;
  font-size:24rpx ;
  color: #939393;
}
.share_content{
  background-color: #ffffff;
  margin-bottom: 16rpx;
}
.share_content_head{
  padding: 0 25rpx;
  height: 90rpx;
  line-height: 90rpx;
  border-bottom: 1px solid #d9d9d9;

}
.left_userInfo{
  float: left;
}
.left_userInfo image{
      width: 55rpx;
      height: 55rpx;
      vertical-align: middle;
      border-radius: 50%;
}

.left_userInfo text{
  font-size: 30rpx;
  color: #333333;
  margin-left:17rpx; 
}
.right_price{
  float: right;
  font-size:30rpx ;
  color:#333333 ;
}
.share_detail_item{
  overflow: hidden;
  width: 100%;
  line-height: 50rpx;
}
.Shitem_left{
  float: left;
  font-size: 26rpx;
  color: #666666;
}
.share_detail{
  padding: 25rpx;
}
.Shitem_right{
  font-size: 22rpx;
  color: #333333;
  float: right;
}
.Shitem_right .Shrt1{
  font-size:26rpx ;
}

.Shitem_right .Shrt2{
  font-size:30rpx ;
  color: #ff7700;
}

.get_detailContetn{
  padding: 0 25rpx;
  background-color: #fff;
}
.detailContent{
  padding: 25rpx 0;
  border-bottom: 1px solid #d9d9d9;
  overflow: hidden;
}
.detailContent_left{
  float: left;
}
.detailContent_left view:first-child{
  font-size: 28rpx;
  color: #333333;
}
.detailContent_left view:last-child{
  font-size: 24rpx;
  color: #999999;
  margin-top: 15rpx;
}
.get_detailItem:last-child{
  margin-bottom: 80rpx;
}
.detailContent_right{
  font-size: 32rpx;
  color: #ff7700;
  float: right;
  margin-top: 15rpx;
}


















