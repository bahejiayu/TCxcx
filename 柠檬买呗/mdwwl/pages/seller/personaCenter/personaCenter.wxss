page{
  background-color: #f4f4f4;
  border-top: 1px solid #f4f4f4;
}
.centerTop{
  background-color: #fff;
  padding: 40rpx 25rpx;
  overflow: hidden;
  margin-bottom: 18rpx;
}

.cenHeadICon{
  width:135rpx ;
  border-radius: 50%;
  overflow: hidden;
  height: 135rpx;
  float: left;
}
.cenHeadICon image{
    width: 100%;
    height: 100%;
}
.TopContetn{
  float: left;
  margin-left: 20rpx;
  padding-top: 20rpx;
}

.TopContetn view:first-child{
    font-size:38rpx ;
    color: #191919;
}
.TopContetn view:last-child{
    font-size:26rpx ;
    color: #999999;
    margin-top: 20rpx;
}


.UserNavContainer{
    background-color: #fff;
    padding-left: 25rpx;
    margin-bottom: 25rpx;
}
.UserNavItem{
  height: 99rpx;
  border-bottom: 1px solid #eeeeee;
  padding: 0 25rpx 0 30rpx;
  line-height: 99rpx;
  font-size: 30rpx;
  position: relative;
  color: #333333;
  overflow: hidden;
}
.UserNavItem image{
  vertical-align: middle;
  width: 40rpx;
  margin-right: 30rpx;
  height: 40rpx;
}
.icRight{
  position: absolute;
  right: 25rpx;
  color: #c7c7cc;
  top: 50%;
  transform: translateY(-50%);
}








