page{
  background-color: #f4f4f4;
}
.geCInfo{
  height: 375rpx;
  background-color: #fff;
  text-align: center;
  box-sizing: border-box;
  margin-bottom:15rpx ;
  padding: 70rpx 0;
}
.geCInfo view:first-child{
  font-size: 36rpx;
  color: #333333;
}
.geCInfo view:nth-of-type(2){
  font-size: 60rpx;
  color: #ff3f3f;
  margin: 25rpx 0;
}
.btnOnce{
  width:280rpx ;
  height:70rpx ;
  text-align: center;
  line-height: 70rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #ffffff;  
   background-image: -webkit-linear-gradient(top, #ff853c, #ff6e5d); 
   border-radius: 30rpx;
}
.moneyBox{
  padding: 25rpx;
  background-color: #fff;
  overflow: hidden;
  margin-bottom: 15rpx;
}
.moneyItem{
  float: left;
  width: 33.33%;
  text-align: center;
  box-sizing: border-box;
}
.moneyItem view:first-child{
    font-size:24rpx ;
    color: #999999;

}
.moneyItem view:last-child{
    font-size:32rpx ;
    margin-top: 10rpx;
    color: #ff3f3f;

}
.moneyBox .moneyItem:nth-of-type(2){

  border-left: 1px solid #dcdcdc;
  border-right: 1px solid #dcdcdc;
}
.getNav{
  height:78rpx ;
  line-height: 78rpx;
  border-bottom: 2rpx solid #e5e5e5;
  background-color: #fff;
  padding: 0 25rpx;
}
.getNav view{
  float: left;
  font-size: 28rpx;
  color: #1e1e1e;
  font-weight: bold;
  padding: 0 12rpx;
  margin-right: 40rpx;
}
.getNav .cur{
  color: #ffda43;
  border-bottom: 3rpx solid #ffda43;

}

.withTop{
  height: 400rpx;
  background-image: -webkit-radial-gradient(0px 0px, #1094f0, #2da8fc); 
  position: relative;
}
.TopCenter{
  text-align: center;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.TopCenter view:first-child{
  font-size: 60rpx;
  color: #FFF;
  margin-bottom: 5rpx;
}
.TopCenter view:first-child text{
  font-size: 30rpx;
}
.TopCenter view:last-child{
  font-size: 26rpx;
  color: #FFF;
}
.all_get{
  color: #fff;
  font-size: 24rpx;
  height: 60rpx;
  padding: 0 18rpx;
  border: 1px solid #fff;
  border-radius: 10rpx;
  line-height: 60rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.bottomNav{
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 108rpx;
}

.bottomItem{
  float: left;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #fff;
  width:33.33%;
  text-align: center;
}
.line_b{
  width: 33.33%;
  position: absolute;
  height: 8rpx;
  bottom: 0;
  left: 0;
  transition: all 0.4s;
  background-color: #ffb80f;
}
page{
  background-color: #eeeeee;
}
.toDay{
  height: 178rpx;
  line-height: 178rpx;
  background-color: #fff;
  border-bottom: 2rpx solid #cccccc;
  /* margin-top: 10rpx; */
  padding: 0 25rpx;
}
.toDay view:first-child{
  float: left;
  font-size:30rpx ;
  color: #333;
}
.toDay view:last-child{
  float: right;
  font-size:32rpx ;
  color: #f9484d;
}
.moneyDetail{
  overflow: hidden;

}
.money_item{
  width: 33.33%;
  text-align: center;
  padding: 60rpx 0;
  float: left;
}
.money_item view:first-child{
  font-size: 44rpx;
  color:#ff7700 ;
}

.money_item view:last-child{
  font-size: 24rpx;
  margin-top: 15rpx;
  color:#666666 ;
}
.go_getnow{
  display: block;
  width:140rpx ;
  height:55rpx ;
  text-align: center;
  line-height: 55rpx;
  margin: 0 auto;
  font-size: 30rpx;
  color: #0da056;
  border: 2rpx solid #0da056;
  border-radius: 10rpx;
  position: relative;
  z-index: 1000;
}
.nav_Detail{
    height: 92rpx;
    line-height: 92rpx;
    background-color: #fff;
    font-size: 30rpx;
    color:#333333 ;
    position: relative;

}

.nav_item{
  float: left;
  width: 50%;
  text-align: center;
}

.line_gr{
  position: absolute;
  width: 50%;
  bottom: 0;
  height: 4rpx;
  background-color: #0da056;
  left: 0;
  transition: all 0.2s;
}

