.topSearch{
    padding: 13rpx 25rpx;
    border: 2rpx solid #f4f4f4;
    background-color: #fff;
}
.searchContainer{
  height: 65rpx;
  background-color: #f1f1f1;
  border-radius: 10rpx;
  position: relative;
  padding-left: 60rpx;
}
.hbdk{
  font-size: 24rpx;
  color: #fd6906;
  padding-top: 5rpx;
}
.searchBtn{
  width: 60rpx;
  height: 65rpx;
  text-align: center;
  line-height: 65rpx;
  position: absolute;
  left: 0rpx;
  font-size: 32rpx;
  color: #999999;

}
.searchContainer input{
  width: 100%;
  height: 65rpx;
  line-height: 65rpx;
  box-sizing: border-box;
  padding-right:20rpx; 
  font-size: 30rpx;

}

.inclass{
      color: #828282;
}
.fenNav{
    padding: 17rpx 0;
    padding-left: 25rpx;
    background-color: #fff;
}
page{
  background-color: #f4f4f4;
}
.fenNav{
  white-space: nowrap;
}
.fenNav view{
  display: inline-block;
  padding: 10rpx 25rpx;
  font-size: 26rpx;
  background-color: #f1f1f1;
  border-radius: 10rpx;
  margin-right: 25rpx;
  color: #828282;
}
.fenNav .cur{
  color: #fff;
  background-color: #ffda43;
}

.shop_list{
  background-color: #f4f4f4;
  padding-bottom: 50rpx;
  margin-top: 15rpx;
}
.shop_item{
  background-color: #fff;
  border-bottom: 15rpx;
  position: relative;
  padding: 25rpx;
  padding-right: 225rpx;
  height: 90rpx;
  margin-bottom: 15rpx;
}
.shop_itmeLeft{
  position: relative;
  padding-left: 112rpx;

}
.shop_itmeLeft image{
  width:90rpx ;
  height: 90rpx;
  border-radius: 10rpx;
  position: absolute;
  left: 0;
}
.shop_name{
  font-size:32rpx ;
  color:#333 ;
  /* padding-top: 5rpx; */

}
.shop_info{
  font-size:24rpx ;
  color: #ababab;
  margin-top: 7rpx;
}
.shop_info text{

  margin:0 30rpx; 
}
.shop_itemRight{
  position: absolute;
  right: 25rpx;
  top: 25rpx;
  font-size: 24rpx;
  color: #333;
}
.shop_itemRight view:first-child{
  padding-top: 10rpx;
  text-align: right;
}
.hot_list{
  text-align: right;
  margin-top: 10rpx;

}
.hot_list image{
  width: 22rpx;
  height: 22rpx;
  margin-left: 10rpx;
}
.shop_itemRight view:first-child{
  padding-top: 10rpx;
}













