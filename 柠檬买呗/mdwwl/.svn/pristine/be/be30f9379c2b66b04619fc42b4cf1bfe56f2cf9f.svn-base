.fromBox{
  padding-left: 30rpx;
  background-color: #fff;

}
page{
  background-color: #f1f1f1;
}
.from_item{
  position: relative;
  height: 98rpx;
  line-height: 98rpx;
  border-bottom: 2rpx solid #e9e9e9;
  padding-left: 175rpx;
}
.from_title{
  position: absolute;
  left: 0;
  top: 0;
  font-size:32rpx ;
  color:#000000 ;
}
.from_item input{
  font-size: 32rpx;
  height: 100%;
  width: 100%;
}
.from_item .icon-right{
  position: absolute;
  top: 0;
  right: 30rpx;
  transform: rotate(90deg);
}

.fromBox .from_item:last-child{
  border-bottom: none;
}

.fromBox{
  border-bottom: 2rpx solid #dcdcdc;
  margin-bottom: 20rpx;
}
.codeBtn{
  padding: 0 30rpx;
  height: 65rpx;
  line-height: 65rpx;
  background-color: #0097ff;
  color: #fff;
  font-size: 30rpx;
  position: absolute;
  right: 30rpx;
  top: 50%;
  transform: translateY(-50%);
  border-radius: 10rpx;
}

.fromTip{
  padding: 0 30rpx;
  font-size: 24rpx;
  color: #a2a2a2;
}
.btnBox{
  padding:50rpx   30rpx;
}










