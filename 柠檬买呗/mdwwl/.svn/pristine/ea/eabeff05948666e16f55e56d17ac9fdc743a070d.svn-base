  background-color: #f6f7f9;
  box-sizing: border-box;
}
.cancel_txt{
  width:70rpx ;
  height: 68rpx;
  line-height: 68rpx;
  text-align: right;
  font-size: 26rpx;
  position: absolute;
  right: 0;
  top: 0;
  color: #323233;
}
.search_box{
  position: relative;
  padding-left: 65rpx;
}


.search_conteiner{
  position: absolute;
  left: 0rpx;
  text-align: center;
  top: 0;
  width: 65rpx;
  font-size: 28rpx;
}
.search_box input{
  width: 100%;
