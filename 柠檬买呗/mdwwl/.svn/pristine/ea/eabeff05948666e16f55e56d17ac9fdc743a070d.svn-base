}
.goods_list{
  padding-bottom: 50rpx;  
}
.goods_item{

  padding: 25rpx;
  background-color: #fff;
  border-bottom: 1px solid #dcdcdc;
  height: 225rpx;
  padding-left: 280rpx;
  position: relative;
}
.goods_img{
  position: absolute;
  left: 25rpx;
  width: 225rpx;
  height:225rpx ;
}
.goods_item navigator{
  display: block;
  width: 100%;
  height: 100%;
}
.goods_img image{
  width: 100%;
  height: 100%;
}

.goods_name{
  font-size: 30rpx;
  line-height: 44rpx;
  color: #484848;
   text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.name_height{
  height: 115rpx;

}
.goods_price{
  font-size: 30rpx;
  color: #f1002d;
}
.once_buy{
  width: 160rpx;
  height:56rpx ;
  line-height: 56rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background-color: #f49629;
  border-radius: 5rpx;
  float: right;
  margin-top: 20rpx;
}
.fix_tip{
  position: fixed;
  opacity: 0;
  height: 55rpx;
  line-height: 55rpx;
  background-color: rgba(0, 0, 0, 0.6);
  padding:8rpx 10rpx ;
  font-size:26rpx ;
  color: #fff;
  border-radius: 40rpx;
  top: 45rpx;
  left: 25rpx;
}

.fix_tip image{
  width:55rpx ;
  height: 55rpx;
  border-radius: 50%;
  margin-right: 15rpx;
  vertical-align: middle;
}
.fix_tip text{
  margin-right: 10rpx;
}
.animated {
    -webkit-animation-duration: 1s;
    animation-duration: 1s;
    -webkit-animation-fill-mode: both;
    animation-fill-mode: both
}

.animated.infinite {
    -webkit-animation-iteration-count: 1;
    animation-iteration-count: 1
}










