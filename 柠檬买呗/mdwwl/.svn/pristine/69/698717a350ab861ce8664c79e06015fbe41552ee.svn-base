page{
  background-color: #ffffff;
}
.top_img{
  height: 585rpx;
  position: relative;
  margin-bottom: 250rpx;
}
.top_img .bg{
  width: 100%;
  height: 100%;
}
.top_img .code_{
  position: absolute;
  width: 450rpx;
  height:450rpx;
  box-sizing: border-box;
  padding: 30rpx;
  background-color: #fff; 
  border-radius: 10rpx;
  bottom: 0;
  left: 50%;
  box-shadow: 0rpx 0rpx 20px #dcdcdc;
  transform: translate(-50%,50%);
}
.code_ image{
  width: 100%;
  height: 100%;
}

.code_txt{
  text-align: center
  
}
.code_txt view:first-child{
  font-size: 36rpx;
  color: #212121;
  margin-bottom: 25rpx;
}
.code_txt view:last-child{
  font-size: 30rpx;
  color: #666666;
}

.btns{
  /* margin-top: 50rpx; */
}
.btns button{
  width: 600rpx;
  text-align: center;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 32rpx;
  color: #333333;
  border: 2rpx  solid #333333;
  border-radius: 40rpx;
  margin: 0 auto;
  margin-bottom: 30rpx;
}
.btns button:first-child{
  background-color:#ffda43 ;
  border: 2rpx  solid #ffda43;

}
.bg_img{
  width: 100%;
}
.btns{
  padding-bottom: 40rpx;
}









