page{
  background-color: #ffffff;
  border-top: 1px solid #dcdcdc;
}
.verifContainer{
  padding: 0 25rpx;
}
.verItem{
    height: 110rpx;
    border-bottom: 1px solid #dcdcdc;
    padding-left: 160rpx;
    padding-right: 210rpx;
    position: relative;

}
.verTitle{
  line-height: 110rpx;
  font-size: 34rpx;
  position: absolute;
  left: 0;
  color: #333333;
}
.verItem input{
  height: 110rpx;
  font-size: 34rpx;
  color: #333333;

}
.get_Code{
  position: absolute;
  right: 20rpx;
  top: 50%;
  transform: translateY(-50%);
  font-size:34rpx ;
  color:#41b91a ;

}
.verBox{
  margin-top: 75rpx;
}
.btn{
  height: 95rpx;
  line-height: 95rpx;
  text-align: center;
  border-radius: 10rpx;
  margin-top: 70rpx;
  color: #fff;
  background-color: #ffda43;
  font-size: 34rpx
}



















