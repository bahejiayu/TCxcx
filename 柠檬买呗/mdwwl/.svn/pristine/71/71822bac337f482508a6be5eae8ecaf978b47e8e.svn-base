}
.zz{
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: #000;
  opacity: 0;
  top: 0;
  left: 0;
  z-index: 100;
  transform: all 1s;
}
.op6{
  opacity: 0.6!important;

}
.range_box{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 760rpx;
  background-color: #fff;
  z-index: 1000;
  transform: translateY(150%);
  transition: all 0.4s;
}
.tr0{
  transform: translateY(0)!important;
}

.range_top{
  height: 660rpx;
  padding: 0  30rpx;
}
.range_top_one{
  padding-left: 260rpx;
position: relative;
border-bottom: 1px solid #dcdcdc;
}
.range_top_one{
  height: 180rpx;
  padding-bottom: 29rpx;
}
.range_goods_img{
  width:228rpx ;
  height: 228rpx;
  border: 1px solid #dcdcdc;
  background-color: #f5f5f5;
  left: 0;
  position: absolute;
  top: -50rpx;
  border-radius: 10rpx;
  overflow: hidden;
}
.range_goods_img image{
  width: 100%;
  height: 100%;
}
.range_price{
  font-size: 40rpx;
  color: #c91d1d;
  padding: 35rpx 0 15rpx 0;
}
.range_price text{
  font-size: 28rpx;
}
.alear_chk{
  font-size: 26rpx;
  color: #000000;
}
.range_list{
  padding-bottom: 20rpx;
  border-bottom: 1px solid #dcdcdc;
}
.range_title{
  height: 74rpx;
  font-size:26rpx ;
  color: #000000;
  line-height: 85rpx;
  overflow: hidden;
}
.range_container{
  overflow: hidden;
}
.range_container .range_item{
  float: left;
  padding: 0 19rpx;
  height:55rpx ;
  font-size:26rpx ;
  line-height: 55rpx;
  border-radius: 10rpx;
  background-color: #f6f6f6;
  margin-right: 25rpx;
  color: #333333;

}
.range_container .range_item.cur{
background-color: #f1962c;
  color: #fff;
}
.range_nubBox{
  padding: 30rpx 0;
  border-bottom: 1px solid #dcdcdc;
  position: relative;
  overflow: hidden;
}
.range_nubBoxTitle{
  position: absolute;
  left: 0;
  top: 50%;
  transform: translateY(-50%);
  font-size: 26rpx;
  color: #000000;
}
.nub_container{
  float: right;
  height: 50rpx;
}

.nub_container view{
  float: left;
  background-color: #f2f2f2;
  position: relative;
  margin-left: 5rpx;
}
.down_nub{
  width: 60rpx;
  height: 50rpx;
}
.down_nub::before{
  content: "";
  width: 22rpx;
  height: 4rpx;
  background-color: #58595b;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.add_nub{
  width: 60rpx;
  height: 50rpx;
}
.add_nub::before{
  content: "";
  width: 22rpx;
  height: 4rpx;
  background-color: #58595b;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.add_nub::after{
  content: "";
  width: 4rpx;
  height: 22rpx;
  background-color: #58595b;
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%,-50%);
}
.nub_value{
  width: 100rpx;
  height: 50rpx;
}


.nub_value input{
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  text-align: center;
}

.op::before{
  opacity: 0.6;
}
.sure_btn{
  height: 100rpx;
  background-color: #f1962c;
  font-size: 34rpx;
  color: #fff;
  display: flex;
  justify-content: center;
  align-content: center;
  line-height: 100rpx;
}







