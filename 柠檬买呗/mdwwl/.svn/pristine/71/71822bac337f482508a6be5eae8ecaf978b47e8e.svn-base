.goods_swiper{
  height: 750rpx;
  position: relative;
}
.goods_swiper swiper{
  height: 100%;
}
.goods_swiper image{
  width: 100%;
  height: 100%;
}
.swiper_nub{
  position: absolute;
  height:32rpx;
  padding: 0 10rpx;
  font-size: 24rpx;
  color: #333333;
  background-color: #ffffff;
  right: 25rpx;
  bottom: 30rpx;
  border-radius: 5rpx;
}
.goods_ifno{
  background-color: #fff;
  padding: 0 25rpx;
  margin-bottom: 20rpx;
}
.goods_price{
  overflow: hidden;
}
.goods_price view{
  font-size:48rpx ;
  display: inline-block;
  color: #d4282d;
}
.goods_price view:first-child{
  font-size: 32rpx;
  vertical-align: text-bottom;
}
page{
  background-color: #f5f5f5;
}
.goods_name{
  font-size:30rpx ;
  line-height: 48rpx;
  margin-top: 20rpx;
  color: #252525;
  padding-bottom: 30rpx;
}

.goods_ifno>view:first-child{
 padding-top: 25rpx;
 position: relative;
}
.people_buy image{
  width:32rpx;
  height: 32rpx;
  vertical-align: middle;
}
.people_buy{
  font-size:26rpx ;
  color: #a7a7a7;
  position: absolute;
  right: 0;
  top: 30rpx;

}
.line_box{
  background-color: #fff;
  margin-bottom: 20rpx;
}

.line_item{
  padding: 0 30rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333333;
} 
.line_item:first-child{
  border-bottom: 2rpx solid #f1f1f1;
}
.line_item >view:first-child{
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #333333;
  float: left;
} 

.line_item .icon-right{
    float: right;
    font-size: 30rpx;
    color: #333333;

}
.line_item >view:first-child text{
  color: #a4a4a4;
  margin-right: 28rpx;
}
.images_des{
  padding-bottom: 100rpx;
}
.image_list{
  background-color: #fff;
}
.des_title{
  text-align: center;
  height: 99rpx;
  line-height: 99rpx;
  font-size: 32rpx;
  color: #333333;
  background-color: #fff;
  border-bottom: 1px solid #dcdcdc;
}
.des_title view{
  display: inline-block;
}
.des_title view:first-child{
  width:90rpx ;
  height: 2rpx;
  background-color: #d2d2d2;
  vertical-align: middle;

}
.des_title view:last-child{
  width:90rpx ;
  height: 2rpx;
  background-color: #d2d2d2;
  vertical-align: middle;
}

.des_title view:nth-of-type(2){
  margin: 0 27rpx;
}
.image_list image{
  width: 100%;
}
.fixed_bottom{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: 100rpx;
  border-top:1px solid #d9d9d9;
  padding-left: 95rpx;
  background-color: #f1962c;
  box-sizing: border-box;

}
button{
  background-color: transparent;
  line-height: 1;
  font-size:24rpx;
color:#494949;
padding: 0;
}
button::after{
    border: 0;
}
.kefu_{
