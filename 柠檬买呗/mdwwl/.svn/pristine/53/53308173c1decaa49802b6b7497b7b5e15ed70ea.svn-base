page {
  background-color: #f5f5f5;
}

.top_nav {
  height: 92rpx;
  border-top: 1px solid #dcdcdc;
  background-color: #fff;
  font-size: 28rpx;
  color: #7a7a7a;
  display: flex;
  overflow: hidden;
}
.nav_item {
  flex: 1;
  text-align: center;
  height: 87rpx;
  line-height: 102rpx;
}
.nav_item.cur {
  font-size: 30rpx;
  color: #1e1e1e;
  border-bottom: 5rpx solid #ffda43;
}
.order_item{
  background-color: #fff;
  margin-top: 20rpx;
  padding-left: 30rpx;
}
.order_title{
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 2rpx solid #eeeeee;
  font-size: 28rpx;
  padding-right: 30rpx;
}
.order_title .order_no{
  float: left;
  color: #454545;  
}
.order_title .order_status{
  float: right;
  color: #fc2934;  
}
.order_goods{
  padding: 23rpx 0;
  height: 140rpx;
  padding-left: 160rpx;
  border-bottom: 2rpx solid #eeeeee;
  position: relative;
}
.order_image{
  width: 140rpx;
  height: 140rpx;
  background-color: #f1f1f1;
  position: absolute;
  left: 0;
}
.order_image image{
  width: 100%;
  height: 100%;
}
.goods_des{
  line-height: 50rpx;
  font-size: 30rpx;
  color: #333;
  overflow: hidden;

}
.order_goods_info{
  padding-right: 30rpx;
}
.goods_name{
  float: left;
  width: 60%;
}
.goods_price{
  float: right;
  width: 40%;
  text-align: right;
}
.goods_range{
  font-size:26rpx ;
  color: #999999;
  margin-top: 10rpx;
}
.goods_range_left{
  float: left;
}
.goods_range_right{
  float: right;
}
.order_footer{
  padding: 17rpx 0;
  padding-right: 30rpx;
}
.order_footer{
  text-align: right;
 
}
.order_footer navigator{
 display: inline-block;
  height: 60rpx;
  padding:0  27rpx ;
  box-sizing: border-box;
  font-size:26rpx ;
  margin-left: 20rpx;
  line-height: 60rpx;
  border: 1px solid #818181;
  border-radius: 10rpx;
  color:#333333 ;
}
.order_footer .type_{
  background-color: #ffda43;
  border: 1px solid #ffda43;

}


