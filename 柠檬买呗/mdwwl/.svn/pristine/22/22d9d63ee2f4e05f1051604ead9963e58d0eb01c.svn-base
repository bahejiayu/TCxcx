page{
  background-color: #f4f4f4;
}
.moey_icon{
  padding-top: 130rpx;
  text-align: center;
}
.moey_icon image{
  width: 320rpx;
  height:147rpx ;
}
.money_box {
  margin-top: 25rpx;
}
.money_box view{
  text-align: center;
}
.money_box view:first-child{
  font-size:30rpx ;
  color: #333333;
}
.money_box view:last-child{
  font-size:60rpx ;
  color: #161616;
  margin-top: 25rpx;
}
.btnBox{
  margin-top: 30rpx;
  padding: 55rpx 25rpx;
}



.btnBox button{
  background-color: #07c160;
}
.nav_list{
  background-color: #fff;
  border-top: 1px solid #ebedf0;
  border-bottom: 1px solid #ebedf0;
  padding-left: 25rpx;
}

.nav_item{
  height: 85rpx;
  line-height: 85rpx;
}
.nav_item:first-child{
  border-bottom: 2rpx solid #f5f6f7;
}
.nav_item view{
  float: left;
  font-size: 28rpx;
  color: #323233;
}
.nav_item view:last-child{
  float: right;
  margin-right: 25rpx;
  font-size: 35rpx;
  color: #969799;
}
.nav_item navigator{
  display: block;
  width: 100%;
  height: 100%;
}

