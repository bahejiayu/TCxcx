page{
  background-color: #f4f4f4;
}
.shopItem{
  margin-top: 25rpx;
  background-color: #fff;
}
.shopItem_top{
  padding: 25rpx;
  border-bottom: 2rpx solid #f4f4f4;
}
.shopItem_topShopInfo{
  height: 90rpx;
  padding-left: 110rpx;
  position: relative;
}
.shopLogo{
  position: absolute;
  width: 90rpx;
  height: 90rpx;
  left: 0;
  border-radius: 10rpx;
  overflow: hidden;
  top: 0;
}
.shopLogo image{
  width: 100%;
  height: 100%;
}
.shopdes view:first-child{

    font-size:32rpx ;
    color: #151515;
}

.shopdes view:last-child{
  font-size:24rpx ;
  margin-top: 8rpx;
  color: #666666;
}
.shopdes view:last-child text{
  color: #fd6906;
  margin-right: 30rpx;
}
.shopItem_bottom{
  padding: 15rpx 25rpx;
  text-align: right;
  overflow: hidden;
}
.shopItem_bottom navigator{
  font-size: 28rpx;
  float: right;
}
.shopItem_bottom .bj{
  width:96rpx ;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  border: 2rpx solid #d2d2d2;
  color: #333333;
  margin-right: 20rpx;
  border-radius: 10rpx;
}
.shopItem_bottom .jy{
  width:160rpx ;
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color: #fd6906;
  color: #fff;
  border-radius: 10rpx;
}




















