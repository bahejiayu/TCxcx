page{
  border-top: 1px solid #e5e5e5;
}
.goods_info{
  padding: 28rpx 25rpx;
  height: 150rpx;
  position: relative;
  padding-left: 197rpx;
  border-bottom: 1px solid #e5e5e5;
  margin-bottom: 20rpx;
}
.goods_image{
  width: 150rpx;
  height: 150rpx;
  border: 1px solid #e5e5e5;
  background: #f5f5f5;
  left: 25rpx;
  position: absolute;
}
.goods_image image{
  width: 100%;
  height: 100%;
}
.order_status{
  font-size: 28rpx;
  color:#000000 ;
}

.order_status text{
  color: #ee3131;
  margin-left: 20rpx;
}
.order_des{
  font-size:26rpx ;
  margin-top: 10rpx;
  color: #999999;
}
.order_des text{
  margin-left: 20rpx;
}
.order_des view{
  line-height: 35rpx;
}
.log_title{
  padding: 0 25rpx;
  height: 90rpx;
  line-height:90rpx ;
  border-bottom: 1px solid #dcdcdc;
  font-size: 28rpx;
  color: #000000;
}
.log_title image{
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-right: 15rpx;
}
.log_box{
  padding: 0rpx 25rpx;
  padding-left: 57rpx;
  padding-bottom: 60rpx;
}
.log_container{
  padding-left: 95rpx;
}

.log_item{
  padding: 20rpx 0;
  border-bottom: 1px solid #dcdcdc;
  position: relative;
}
.log_item{
  padding-top: 0;
  margin-top: 20rpx;
}

.log_container{
   border-left: 2rpx solid #dddddd;
}
.log_item view:first-child{
  font-size:28rpx ;
  color: #999999;
  line-height: 42rpx;
}
.log_item view:last-child{
  font-size: 24rpx;
  margin-top: 15rpx;
  color: #999999;
}
.log_item .log_stus{

    width:16rpx ;
    height: 16rpx;
    border-radius: 50%;
    position: absolute;
    left: -95rpx;
    background-color: #dddddd;
    top: 0;
    transform: translateX(-50%);
}
.log_item.cur view{
    color: #000000;
}
.log_item.cur .log_stus{
  width: 32rpx;
  height: 32rpx;
  background-color: #fdedb1;
  display: flex;
  justify-content: center;
}
.log_item.cur .log_stus view{
  width: 20rpx;
  height: 20rpx;
  background-color: #f9d23b;
  border-radius: 50%;
  transform: translateY(-50%);
  /* position: absolute;
    left: 0;
    top: 0; */
}









