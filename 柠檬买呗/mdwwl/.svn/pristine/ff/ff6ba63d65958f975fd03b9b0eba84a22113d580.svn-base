page{
  background-color: #f5f5f5;
}
.top_search{
  padding: 20rpx 25rpx;
  background-color: #fff;
}

.search_container{
  height: 68rpx;
  line-height: 68rpx;
  padding-right: 70rpx;
  position: relative;
}
.search_box{
  width: 100%;
  height: 100%;
  background-color: #f6f7f9;
  box-sizing: border-box;
}
.cancel_txt{
  width:70rpx ;
  height: 68rpx;
  line-height: 68rpx;
  text-align: right;
  font-size: 26rpx;
  position: absolute;
  right: 0;
  top: 0;
  color: #323233;
}
.search_box{
  position: relative;
  padding-left: 65rpx;
}


.search_conteiner{
  position: absolute;
  left: 0rpx;
  text-align: center;
  top: 0;
  width: 65rpx;
  font-size: 28rpx;
}
.search_box input{
  width: 100%;
  height: 100%;
  font-size: 28rpx;
}
.goods_list{
  padding-bottom: 50rpx;  
}
.goods_item{

  padding: 25rpx;
  background-color: #fff;
  border-top: 1px solid #dcdcdc;
  height: 225rpx;
  padding-left: 280rpx;
  position: relative;
}
.goods_img{
  position: absolute;
  left: 25rpx;
  width: 225rpx;
  height:225rpx ;
}
.goods_item navigator{
  display: block;
  width: 100%;
  height: 100%;
}
.goods_img image{
  width: 100%;
  height: 100%;
}

.goods_name{
  font-size: 30rpx;
  line-height: 44rpx;
  color: #484848;
   text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.name_height{
  height: 115rpx;

}
.goods_price{
  font-size: 30rpx;
  color: #f1002d;
}
.once_buy{
  width: 160rpx;
  height:56rpx ;
  line-height: 56rpx;
  text-align: center;
  font-size: 28rpx;
  color: #fff;
  background-color: #f49629;
  border-radius: 5rpx;
  float: right;
  margin-top: 20rpx;
}












