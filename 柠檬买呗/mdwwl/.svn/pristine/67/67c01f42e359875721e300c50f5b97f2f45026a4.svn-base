page{
  background-color: #f4f4f4;
}
.top_nav{
  height: 89rpx;
  background-color: #fff;
  border-bottom: 1px solid #e5e5e5;
  display: flex;
  position: fixed;
  width: 100%;
  top: 0;
  z-index: 1000;

}
.begin_end{
  z-index: 1000000;
}
.top_nav view{
  font-size: 28rpx;
  color: #282828;
  text-align: center;
  flex: 1;
  height:86rpx ;
  line-height:86rpx ;
}
.top_nav view.cur{
  color: #f6b100;
  border-bottom: 4rpx solid #f6b100;
}
.team_item{
  background-color: #fff;
  margin-top: 15rpx;
 
}
.team_top{
 height: 149rpx;
  box-sizing: border-box;
 padding-left: 140rpx;
 padding-right: 70rpx;
 border-bottom: 1px solid #eeeeee;
 position: relative;
}
.head_img{
  position: absolute;
  width:90rpx ;
  height: 90rpx;
  border-radius: 50%;
  overflow: hidden;
  left: 30rpx;
  top: 30rpx;
}

.head_img image{
  width: 100%;
  height: 100%;
}
.team_top_content{
  padding-top: 25rpx;
}
.team_top_content >view:first-child{
  font-size: 28rpx;
  color: #0a0a0a;
}
.tema_des view{
  font-size: 24rpx;
  color: #a4a3a3;
}
.tema_des{
  margin-top: 5rpx;
}
.donw_icon{
  position: absolute;
  right: 25rpx;
  top: 50%;
  font-size: 40rpx;
  transform: translateY(-50%);
}
.zk{
  transform: translateY(-50%) rotate(90deg);

}
.team_content{
  padding: 10rpx 40rpx;
  padding-bottom: 32rpx;
}
.team_content_item_title{
  font-size: 28rpx;
  font-weight: bold;
  margin-top: 20rpx;
  color: #333333;
}
.data_info{
  margin-top: 10rpx;
}
.data_info view{
  font-size: 28rpx;
  color: #333333;
  line-height: 43rpx;
}





.team_list{
  padding-top: 180rpx;
}

page{
  background-color: #f4f4f4;
}
.top_chk{
  padding: 20rpx 25rpx;
  background-color: #fff;
  overflow: hidden;
   width: 100%;
  top: 90rpx;
  position: fixed;
  z-index: 1000;
  box-sizing: border-box;
}
.top_chk_left view{
  float: left;
  border-radius: 20rpx;
  padding: 0 22rpx;
  height: 50rpx;
  line-height: 50rpx;
  background-color: #f3f3f3;
  font-size: 24rpx;
  margin-right: 20rpx;
  color: #999999;
  box-sizing: border-box;
}
.top_chk_left view.cur{
  background-color: #ffecec;
  color: #ff4444;
  border: 2rpx solid #ff4444;
}

.top_chk_right{
  float: right;
  font-size: 24rpx;
  color: #333333;
  line-height: 50rpx;

}
.top_chk_right view{
    transform: rotate(90deg);
    display: inline-block;
}
.record_txt{
  padding: 0 25rpx;
  height: 65rpx;
  line-height: 65rpx;
  font-size: 26rpx;
  color: #999999;
}
.record_item{
  background-color: #fff;
  margin-bottom: 20rpx;
}
.record_item_title{
  height: 65rpx;
  line-height: 65rpx;
  padding: 0 25rpx;
  font-size: 26rpx;
  color: #575757;
  border-bottom: 2rpx solid #f4f4f4;
}
.record_item_title view:first-child{
  float: left;
}
.record_item_title view:last-child{
  float: right;
}
.record_item_content{
  padding:  25rpx;
  padding-left: 140rpx;
  position: relative;
}

.user_image{
    width:90rpx ;
    height: 90rpx;
    overflow: hidden;
    border-radius: 50%;
  position: absolute;
  left: 30rpx;
}
.user_image image{
  width: 100%;
  height: 100%;

}
.record_item_info view{
  font-size: 26rpx;
  color: #575757;
  line-height: 45rpx;

}
.record_item_info view text{
  color: #f25353;
}
.b{
  font-weight: bold;
}
.begin_end{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  padding: 35rpx  0;
  background-color: #fff;
  z-index: 10000000;
}
.begin_end view{
  width:50%;
  float: left;
  font-size: 26rpx;
  text-align: center;
}
.zz{
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0.6;
  background-color: #333;
  z-index: 10000;
}
.begin_end view.cur{
  color: #f25353;

}



