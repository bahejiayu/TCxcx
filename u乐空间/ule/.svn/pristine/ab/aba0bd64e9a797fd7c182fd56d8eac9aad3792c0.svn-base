/* pages/combo/combo.wxss */
.count {
  /* text-align: right;
   */
   justify-content:flex-end ;
   margin: 10rpx 0;
}
.radio_{
  width: 40rpx;
  height:40rpx ;
  border: 3rpx solid #d2d2d2;
  border-radius: 50%;
  margin-right: 15rpx;
  line-height: 40rpx;
  text-align: center;
  position: relative;
}
.radio_ image{
  width:20rpx ;
  height: 14rpx;
  display: none;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.radio_.cur{
  background-color: #98795d;
}
.radio_.cur image{
  display: inline-block;
}
.goosc{
  padding-bottom: 120rpx;
}

.fixedBottom{
  position: fixed;
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  bottom: 0;
  box-sizing: border-box;
  padding-right: 210rpx;
  z-index: 10000;
}
.right_btn{
  width: 210rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color:#98795d ;
  color: #fff;
  font-size: 34rpx;
  text-align: center;
  position: absolute;
  right: 0;
  top: 0;

}
.fixLeftContent{
  padding: 0 20rpx;
  line-height: 100rpx;
  height: 100rpx;
}
.fixLeftContent .radio_{
  display: inline-block;
  vertical-align: middle;
}
.fixLeftContent>view:first-child{
  font-size: 28rpx;
  color: #909090;
  float: left;
}
.fixLeftContent>view:last-child{
  font-size: 34rpx;
  color: #d81b22;
  float: right;

}
.goosc_title{
  text-align: center;
  font-size:30rpx ;
  height: 90rpx;
  line-height: 90rpx;

}







