page {
	padding-top: 120rpx;
}

list.menu item {
	transform: translate(0rpx, 0);
	transition: all 0.4s ease-in-out 0s;
}

list.menu item.move {
	transform: translate(-200rpx, 0);
}

list.menu item .moveBtn {
	position: absolute;
	width: 200rpx;
	height: 100%;
	display: flex;
	align-items: center;
	justify-content: center;
	right: -230rpx;
}
