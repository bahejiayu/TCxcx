.backHome {
	position: fixed;
	background-color: rgba(0, 0, 0, 0.5);
	border-top-left-radius: 30px;
	border-bottom-left-radius: 30px;
	right: 0;
	bottom: 300rpx;
	color: #fff;
	padding: 20rpx 15rpx;
}

.CountDown {
	display: inline-flex;
	width: 200rpx;
	height: 200rpx;
	border-radius: 50%;
	background-color: #e5e5e5;
	justify-content: center;
	align-items: center;
	flex-direction: column;
	position: relative;
}

.CountDown view {
	position: relative;
	z-index: 3;
}

.circular {
	position: absolute !important;
	width: 100%;
	height: 100%;
  background: #fff;
  border-radius: 50%;
  font-size: 10px;
  border-top: 6rpx solid rgba(0, 0, 0, 0.05);
  border-right: 6rpx solid rgba(0, 0, 0, 0.05);
  border-bottom: 6rpx solid rgba(0, 0, 0, 0.05);
  border-left: 6rpx solid #f37b1d;
  animation: icon-spin 1s infinite linear;
  z-index: -1;
}

.CountDown::after {
	content: "";
	position: absolute;
	width: calc(100% - 4px);
	height: calc(100% - 4px);
	border-radius: 50%;
	top: 2px;
	left: 2px;
	background: #fff;
	z-index: 2;
}
