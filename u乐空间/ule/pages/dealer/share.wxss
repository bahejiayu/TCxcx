.headTop {
  display: flex;
  background-image: url('https://ule.ljq360.com/imgFile/shareTopBg.jpg');
  align-items: center;
  height: 360rpx;
  background-size: 750rpx 360rpx;
  padding: 130rpx 80rpx 70rpx;
}

.qrBox {
  background-color: #f0e8e1;
  padding: 20rpx;
  border: 1px solid #98795d;
  width: 320rpx;
  height: 320rpx;
  border-radius: 10rpx;
  margin: 50rpx auto 0;
}

.qrBox image {
  width: 100%;
  height: 100%;
  display: block;
  background: #fff;
}

