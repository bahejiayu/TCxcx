.FixBottomNav{
  position: fixed;
  bottom: 0;
  width: 100%;
  overflow: hidden;
  padding: 12rpx 0;
  text-align: center;
  font-size: 20rpx;
  color: #838383;
  background-color: #fff;
  border-top: 1px solid #e5e5e5;
}
.Fix_item{
  float: left;
  width: 25%;
}
.Fix_item image{
  width: 44rpx;
  height: 48rpx;
}
.curImg{
  display: none;
}
.cur image{
  display: none;
}
.cur .curImg{
  display: inline-block;
}
.cur view{
  color: #ff970f;
}

