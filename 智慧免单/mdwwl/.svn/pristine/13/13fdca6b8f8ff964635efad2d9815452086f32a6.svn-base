page{
  background-color: #f7f7f7;
  border-top: 1px solid #dcdcdc;
}
.FuTop{
  padding: 35rpx 0;
  background-color: #fff;
  overflow: hidden;
  position: relative;
}
.keLine{
  position: absolute;
  width:2rpx ;
  height: 120rpx;
  background-color: #e5e5e5;
  top: 35rpx;
  left: 50%;
  transform: translateX(-50%);
}
.FuItem{
  float: left;
  width: 50%;
  text-align: center;
  height: 120rpx;
}
.FuItem image{
  width: 68rpx;
  height: 62rpx;
}
.FuItem view{
  font-size:28rpx ;
  color: #333333;
}
.keTime{
  text-align: center;
  line-height: 70rpx;
  background-color: #fdf9d9;
  font-size:26rpx ;
  color: #f56367;
  border-top: 1px solid #c9c9c9;
}
.questionTitle{
    line-height: 70rpx;
    padding: 0 25rpx;
    background-color: #f4f4f4;
    font-size: 30rpx;
    color: #666666;
}
.questionList{
  padding-left: 25rpx;
  padding-bottom: 100rpx;
}
.questionItem{
  padding: 30rpx 0;
  border-bottom: 1px solid #dcdcdc;
  background-color: #ffffff;

}
.questionTop{
  font-size: 32rpx;
  color: #333333;
  padding-right: 115rpx;
  position: relative;
}
.questionContent{
  font-size: 28rpx;
  color: #666666;
  line-height: 50rpx;
  margin-top: 25rpx;
  display: none;
}
.icDown{
    position: absolute;
    right: 25rpx;
    top: 50%;
    transform: translateY(-50%) rotate(90deg);
    transition: all 0.3s;
}
.cur .icDown{
 transform: translateY(-50%) rotate(-90deg);
}
.cur .questionContent{
  display: block;
}












