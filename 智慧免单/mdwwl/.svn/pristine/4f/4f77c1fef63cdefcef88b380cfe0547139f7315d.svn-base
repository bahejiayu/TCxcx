        </navigator>
    </view>
     <view class='feeItem'>
        <navigator url='../ManagementInfo/ManagementInfo' hover-class='none' >
            <image src='/images/fre4.png'></image>
            <view>经营信息</view>
        </navigator>
    </view>
     <view class='feeItem'>
        <navigator url='../../write/write?title=关于我们?type=1' hover-class='none' >
            <image src='/images/fre5.png'></image>
