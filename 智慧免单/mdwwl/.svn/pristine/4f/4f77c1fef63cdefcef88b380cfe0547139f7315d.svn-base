
<import src="../footer/footer" />


<view class='FreeTop'>
  <image src='/images/sebg.png' class='ubg'></image>
  <view class='shopMoneyInfo'>
    <view class='getMoney'>
      <view>{{num1}}</view>
      <view>本月营业总额</view>
    </view>

    <view class='toDay'>
      <view class='toDayItem'>
        <view>{{num2}}</view>
        <view>今日营业额(元)</view>
      </view>
      <view class='toDayItem'>
        <view>{{num3}}</view>
        <view>今日订单笔数(个)</view>
      </view>
    </view>
  </view>
</view>

<view class='freeContainer' >
<view class='freeNavBox'>
