.setTitle{
  height: 60rpx;
  line-height: 60rpx;
  text-align: center;
  background-color:#ededed ;
  font-size: 26rpx;
  border-radius: 5rpx;
  color: #333;
}
.setContainer{
  padding: 20rpx 25rpx;
}
.inpItemTitle{
  font-size: 30rpx;
  margin: 25rpx 0 10rpx 0;
}
.itemBox{
  height: 75rpx;
  line-height: 75rpx;
  font-size:30rpx ;
  color:#191919 ;
  border: 2rpx solid #dcdcdc;
  border-radius: 10rpx;
  position: relative;
}
.pad20{
  padding: 0 20rpx;
}
.icon_{
  position: absolute;
  right: 20rpx;
  font-size: 40rpx;
  top: 0;
  /* transform: translateX(-50%); */
}
.txtCenter{
  text-align: center;
}
.itemBox input{
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  padding:0 20rpx;
  font-size: 30rpx;
}
.getLoction{
  padding-right: 80rpx;

}
.itemBox{
  position: relative;
}
.getLoctImg{
  position: absolute;
  right: -60rpx;
  top: 55%;
  transform: translateY(-50%);
}
.getLoctImg image{
  width: 44rpx;
  height:48rpx ;
}
.areaTxt{
  height:180rpx ;
  border: 2rpx solid #dcdcdc;
  border-radius: 10rpx;
  font-size: 30rpx;
  box-sizing: border-box;
  padding: 20rpx;
}
.workTime input{
    width: 195rpx;
    border: 2rpx solid #dcdcdc;
    border-radius: 10rpx;
    height: 75rpx;
    text-align: center;
}
.workTime{
  border: none!important;
  overflow: hidden;

}
.workItem{
  float: left;
}
.workItem:nth-of-type(2){
  margin: 0 20rpx;
}
.up_box{
  width:180rpx ;
  height:180rpx ;
  border: 2rpx solid #dcdcdc;
  border-radius: 10rpx;
}
.noUp{
  /* position: relative; */
  display: flex;
  justify-content: center;
  align-items: center;
}
.noUp image{
    width: 57rpx;
    height: 49rpx;
}
.hasUp image{
  width: 100%;
  height: 100%;
}
.TableBtn{
  margin-top: 65rpx;
  margin-bottom: 40rpx;
}
.img_list{
  padding: 25rpx 0;
  overflow: hidden;
}
.img_list image{
    width: 140rpx;
    height: 140rpx;
}
.inpItemTitle text{
  color: #999;
  font-size: 24rpx;
}
.img_item{
  float: left;
  margin-right: 25rpx;
  border: 4rpx dashed #ff970f;
  border-radius: 10rpx;
  position: relative;
}
.del_img{
  position: absolute;
  background-color: #ff970f;
  color: #fff;
  font-size: 26rpx;
  padding: 0 10rpx;
  opacity: 0.8;
  right: 0;
  bottom: 0;
}

.upTxt{
  line-height:130rpx;
  float: left;
}