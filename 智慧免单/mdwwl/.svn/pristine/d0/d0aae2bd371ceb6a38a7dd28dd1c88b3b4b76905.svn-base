page{
  background-color: #f4f4f4;
  border: 1px solid #f4f4f4;
}
.goodsContainer{
  padding-bottom: 100rpx;
}
.goodsItem{
  padding: 25rpx;
  border-bottom: 1px solid #e5e5e5;
  background-color: #fff;

}
.goodsBox{
  position: relative;
  padding-left: 225rpx;
  height: 200rpx;
}
.goodsImage{
  width: 200rpx;
  position: absolute;
  left: 0;
  top: 0;
  height:200rpx; 
}
.goodsImage navigator{
   width: 100%;
  height: 100%;
  display: block;
}
.goodsImage image{
  width: 100%;
  height: 100%;
}
.goodsName{
      font-size: 32rpx;
      color:#333333 ;
      padding: 20rpx 0;
}
.goodsPrice{
  font-size: 36rpx;
  color:#f43737 ;
}

.goodsBtn{
  font-size: 30rpx;
  margin-top: 35rpx;
  text-align: right;
}
.goodsBtn text{
  color: #999999;
      margin-left: 28rpx;
}
.goodsBtn text:last-child{
  color: #ff9f31;
}




